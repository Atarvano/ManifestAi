{
  "format_standar": {
    "columns": [
      "item_no",
      "description",
      "hs_code",
      "quantity",
      "unit",
      "unit_price",
      "total_price",
      "weight",
      "volume",
      "country_of_origin",
      "bl_number",
      "marks",
      "seal_number",
      "shipper",
      "shipper_address",
      "consignee",
      "consignee_address",
      "notify_party",
      "notify_address",
      "npwp"
    ],
    "validation_rules": {
      "item_no": "required, numeric",
      "description": "required, string",
      "quantity": "numeric",
      "unit": "string",
      "bl_number": "required, string"
    },
    "transformations": {
      "normalize_units": true,
      "clean_whitespace": true,
      "validate_numbers": true,
      "extract_shipping_info": true
    }
  }
}
